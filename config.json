{
  "detection": {
    "face_scale_factor": 1.1,
    "face_min_neighbors": 5,
    "face_min_size": [30, 30],
    "face_detection_confidence": 0.5,
    "screen_brightness_threshold": 200,
    "screen_min_area": 5000,
    "screen_aspect_ratio_range": [1.0, 2.5],
