    "inappropriate_content_threshold": 0.8,
    "skin_exposure_threshold": 0.15,
    "frame_skip_interval": 2,
    "detection_cache_duration": 30,
    "downscale_factor": 0.3,
    "blur_kernel_size": [21, 21],
    "pixelation_size": 20
  },
  "performance": {
    "target_fps": 120,
    "enable_gpu": true,
    "max_memory_mb": 512,
    "thread_count": 4,
    "enable_caching": true,
    "adaptive_quality": true
  },
  "security": {
    "log_detections": false,
    "encrypt_logs": true,
    "store_faces": false,
    "auto_delete_cache": true,
